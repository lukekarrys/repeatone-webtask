    "qs": "3.1.0",
    "request": "2.56.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-tape-runner": "^2.0.1",
    "git-validate": "^2.1.4",
    "standard": "^10.0.0",
    "tap-spec": "^4.1.1",
